{
	"name": "@acab/ecsstatic",
	"description": "The predefinite CSS-in-JS library for Vite.",
	"version": "0.4.1",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mayank99/ecsstatic.git"
	},
	"author": "Mayank",
	"keywords": [
		"css-in-js",
		"vite",
		"static"
	],
	"type": "module",
	"main": "index.cjs",
	"module": "index.js",
	"types": "index.d.ts",
	"files": [
		"*.js",
		"*.cjs",
		"*.d.ts",
		"!overrides.d.ts"
	],
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./vite": {
			"types": "./vite.d.ts",
			"import": "./vite.js",
			"require": "./vite.cjs"
		}
	},
	"dependencies": {
		"acorn-walk": "^8.2.0",
		"autoprefixer": "^10.4.13",
		"esbuild": "^0.17.5",
		"esbuild-plugin-noexternal": "^0.1.4",
		"magic-string": "^0.27.0",
		"postcss": "^8.4.19",
		"postcss-nested": "^6.0.0",
		"postcss-scss": "^4.0.6"
	},
	"devDependencies": {
		"@types/estree": "^1.0.0",
		"@types/node": "^18.11.18",
		"tsup": "^6.5.0",
		"vite": "^4.0.0"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch"
	},
	"sideEffects": false
}
